<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape>
            <gradient
                android:angle="-90"
                android:startColor="#94D2D6CF"
                android:endColor="#94D2D6CF"
                android:centerColor="#94D2D6CF"
                />
            <size android:height="20dp"
                android:width="20dp"/>
        </shape>
    </item>
    <item android:id="@android:id/progress">
        <clip>
            <layer-list>
                <item>
                    <shape>
                        <gradient
                            android:angle="-90"
                            android:startColor="#FF8F00"
                            android:endColor="#FF8F00"
                            android:centerColor="#FF8F00"
                            />
                    </shape>
                </item>

            </layer-list>
        </clip>
    </item>
</layer-list>